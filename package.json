    "e2e": "ng e2e",
    "log": "git log master --pretty=format:\\\"%an\\\",\\\"%ai\\\",\\\"%s\\\",\\\"%H\\\" > log.csv && echo Commiter, Date, Message, Hash | cat - log.csv > log2.csv && mv log2.csv log.csv",
    "predeploy": "npm run build:prod -- --base-href=https://ansyn.github.io/angular-ansyn-implementation/ --deploy-url=https://ansyn.github.io/angular-ansyn-implementation/",
    "deploy": "gh-pages --repo https://$GITHUB_TOKEN@github.com/AnSyn/angular-ansyn-implementation.git -d dist/angular-ansyn-implementation",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
